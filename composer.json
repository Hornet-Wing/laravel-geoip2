{
    "name": "talkative/laravel-geoip2",
    "description": "Laravel wrapper for MaxMind GeoIP2",
    "keywords": [
        "laravel",
        "geoip",
        "geoip2",
        "maxmind"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jakub Srsen",
            "email": "jsrsen@talkative.uk"
        },
        {
            "name": "Dave Hennigar",
            "email": "dave.hennigar@gmail.com"
        },
        {
            "name": "Csanad Novak",
            "email": "csanad.novak@trendideas.com"
        }
    ],
    "support": {
        "source": "https://github.com/Hornet-Wing/laravel-geoip2",
        "issues": "https://github.com/Hornet-Wing/laravel-geoip2/issues"
    },
    "require": {
        "php": ">=5.4.0",
        "geoip2/geoip2": "~2.0",
        "guzzlehttp/guzzle": "~6.1",
        "illuminate/config": "~4.1|~5.0",
        "illuminate/http": "~4.1|~5.0",
        "illuminate/support": "~4.1|~5.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Talkative\\LaravelGeoIP2\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Talkative\\LaravelGeoIP2\\Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Talkative\\LaravelGeoIP2\\GeoIP2ServiceProvider"
            ],
            "aliases": {
                "GeoIP2": "Talkative\\LaravelGeoIP2\\GeoIP2Facade"
            }
        }
    },
    "minimum-stability": "stable"
}
